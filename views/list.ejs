<%- include('header') %>

<body>
    <div class="box" id="heading">

        <h1><%= listTile %></h1>
    </div>
<div class="box">
    <% for( let i = 0; i < items.length; i++ ) { %>
        <div class="item">
            <input type="checkbox">
            <p><%= items[i] %></p>
        </div>
    <% } %>
    

    <form action="/" method="post" class="item">
        <input type="text" name ='newItem' placeholder="add Item" autocomplete="off">
        <button type="submit" name="list" value=<%=listTile%>>+</button>
    </form>
   
</div>


<%- include("footer") %>

        

  
</body>
</html>